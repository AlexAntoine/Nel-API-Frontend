<%- include('partials/old') -%>

<div class="container mt-5 w-50">

    <h2 class="mb-4">Add Old Device</h2>

    <form action="" method="POST">
        <input type="text" class="form-control" value="ComputerName" name="Computer Name" placeholder="Computer Name">
        <input type="text" class="form-control" value="Manufacturer" name="Manufacturer" placeholder="Manufacturer">
        <input type="text" class="form-control" value="SerialNumber" name="Serial Number" placeholder="Serial Number">
        <input type="text" class="form-control" value="ModelNumber" name="Model Number" placeholder="Model Number">
        <input type="text" class="form-control" value="Age" name="Age" placeholder="Age">
        <input type="text" class="form-control" value="CurrentYear" name="Current Year" placeholder="Current Year">
        <input type="text" class="form-control" value="ShipDate" name="Ship Date" placeholder="Ship Date">
        <input type="text" class="form-control" value="assignedTo" name="Assigned To" placeholder="Assigned To">
        <input type="text" class="form-control" value="notes" name="Notes" placeholder="Notes">
        <button type="submit" class="btn btn-danger btn-block mt-3">Add to Database</button>

    </form>
</div>

<%- include('partials/footer') -%>