<%- include('partials/header') -%>

    <div class="container mt-5">
        <%- include('partials/messages') -%>
        <div id="table1">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">

                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Account Name</th>
                        <th>Username</th>
                        <th>Current Year</th>
                        <th>Operations</th>
                    </tr>
                </thead>

                <tfoot>
                    <tr>
                        <th>Name</th>
                        <th>Account Name</th>
                        <th>Username</th>
                        <th>Current Year</th>
                        <th>Ship Date</th>
                        <th>Operations</th>
                    </tr>
                </tfoot>

                <tbody>
                                          
                <%devices.forEach((device)=>{ %>

                    <tr>
                        <td><%= device.ComputerName%></td>
                        <td><%= device.Age%></td>
                        <td><%= device.SerialNumber%></td>
                        <td><%= device.CurrentYear%></td>
                        <td><%= device.ShipDate%></td>
                        <!-- <td><%= device.CN%></td>
                        <td><%= device.Title%></td>
                        <td><%= device.SamAccountName%></td> -->
                        <!-- <td><%= device.UserPrincipalName%></td> -->
                        <td>
                            <a href="edit/" class="btn btn-primary">Edit</a>
                            <form style="display: inline;" action="/delete" method="POST">
                                <input type="hidden" name="_method" value="DELETE">
                                <button style="cursor:pointer" type="submit" class="btn btn-danger ml-2">Delete</button>
                            </form>
                        </td>
                    </tr>
               <% })%>
                </tbody>

            </table>
        </div>
    </div>

<%- include('partials/footer') -%>